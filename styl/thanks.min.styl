@-webkit-keyframes autofill {
	to {
		color: rgb(164, 163, 163);
		background: #fff;
	}
}

input:-webkit-autofill {
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
}

.button-form
	float left
.alert-message {
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	color: #f00
	margin-left: 130px
}
.checking {
	display: table;
	width 100%
	max-width 367px
	margin: 10px 0 0
	text-align: left
}

.checking .check-label {
	line-height: 20px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 45px;
	text-align: left;
	font-size: 16px
	font-family: AVL
	color #fff
}

.checking .check-label a
	text-decoration: underline
	display: inline
	color #fff
	&:focus
		text-decoration: underline
		color #fff
	&:hover
		text-decoration: none
		color #fff

.checking .check-label:before {
	content: '';
	background-color: red
	position: absolute;
	left: 0;
	top: 3px;
	width: 31px;
	height: 31px;
	border 1px solid #b6b8c0
}

.checking .check-label:after {
	content: '';
	position: absolute;
	left: 10px;
	top: -10px;
	width: 11px;
	height: 18px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 4px solid #47637d
	border-right: 4px solid #47637d
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + .check-label:before {
	background-color: transparent

}

.check:checked + .check-label:after {
	opacity: 1;
	top: 7px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}
