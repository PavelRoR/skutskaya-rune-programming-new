@media screen and (min-width 2295px)
	body
		font-size: 20px
	.container
		width 1400px
	.for-list
		max-width 860px
@media screen and (max-width 1199px)
	.contact
		margin-left: 12px
		&&-email
			padding-left:  32px
		&&-skype
		&&-tel
			padding-left: 27px
@media screen and (max-width 991px)
	#logo
		width 190px
		height 15px
		display block
		float: none
		margin 0 auto 10px
	.contacts
		float: none
		line-height: 16px
		text-align: center
	.contact
		margin 5px 5px 0
		display: inline-block
		span
			padding-left: 5px
	#main
		padding-bottom: 40px
	.pre
		margin-top: 0
	#for
	#last
		padding 40px 0
	#get
		padding 20px 0 15px
@media screen and (max-width 485px)
	header
		margin-bottom: 0
	#logo
		width 160px
		height 14px
		margin-bottom: 5px
	.contact
		line-height: 14px
		font-size: 12px
		&&-email
			padding-left: 20px
		&&-skype
		&&-tel
			padding-left: 17px
	#main
		padding-bottom: 30px
	#for, #last
		padding 30px 0
	#get
		padding 10px 0 5px
	.pre
		font-size: 30px
	.main-title
		margin: 15px 0 10px
	.cost
		font-size: 24px
		margin 10px 0 15px
	.general-title
		font-size: 24px
		margin-bottom: 25px
	.for-list li
		margin-left: 15px
	.button-last
		line-height: 1.3
		padding 15px 10px
		height auto
		margin-top: 30px
	footer h5
		font-size: 18px
	.soc-buttons a
		margin 10px 5px
	footer p
		font-size: 12px
	#conditions
		font-size: 12px
@media screen and (max-width 380px)
	.pre
	.main-subtitle
		font-size: 22px
	.main-title
		font-size: 20px
	.general-title
		font-size: 20px
		margin-bottom: 20px
	.last-text
		font-size: 20px
		margin-bottom: 15px
	.for-list
		padding 15px